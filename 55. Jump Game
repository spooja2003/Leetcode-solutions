class Solution {public boolean canJump(int[] nums) {int index = nums.length - 1;while (index >= 0) {if (nums[index] != 0 || index == nums.length - 1) {index--;continue;}if (nums[index] == 0 && index != nums.length - 1) {boolean doesitpass = false;int possJump = 2; for (int i = index - 1; i >= 0; i--) { if (nums[i] >= possJump) { index = i;doesitpass = true;break;} else {possJump++;}}if (!doesitpass) {return false;}}}return true;}}
